{
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://shinycolors.enza.fun/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://shinycolors.enza.fun/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "injects.js"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "name": "SK4",
  "short_name": "SK4",
  "version": "0.5.0",
  "description": "Shiny Colors translation project",
  "browser_action": {
    "default_popup": "html/index.html"
  }
}
